{
  "all": true,
  "include": ["utils/**/*.js"],
  "exclude": [
    "index.js",
    "test/**",
    "node_modules/**",
    "coverage/**",
    "docs/**"
  ],
  "reporter": ["text", "html", "lcov"],
  "check-coverage": true,
  "lines": 60,
  "functions": 60,
  "branches": 40,
  "statements": 60,
  "report-dir": "coverage"
}
